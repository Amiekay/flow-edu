# Database Configuration
DATABASE_HOST=           # Database host (usually localhost)
DATABASE_PORT=           # Database port (e.g., 5432 or 5431)
DATABASE_NAME=           # Database name
DATABASE_USER=           # Database username
DATABASE_PASSWORD=       # Database password (leave empty if none)

# HD Wallet Mnemonic
# Used for deterministic wallet generation via ethers.js
HD_WALLET_MNEMONIC=      # 12 or 24 word mnemonic phrase (DO NOT share real value)

# Encryption Secret
# Used to encrypt private keys before storing in the database
ENCRYPTION_SECRET=       # Any strong random string
