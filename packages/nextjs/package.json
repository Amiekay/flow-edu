    "kubo-rpc-client": "~5.0.2",
    "luxon": "^3.6.1",
    "next": "~15.2.0",
    "next-nprogress-bar": "~2.3.13",
    "next-themes": "~0.3.0",
    "pg": "^8.13.1",
    "qrcode.react": "~4.0.1",
    "react": "~19.0.0",
    "react-copy-to-clipboard": "~5.1.0",
